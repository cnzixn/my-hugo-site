@charset "UTF-8";.button-container{margin-top:20px;display:flex;flex-direction:column;justify-content:center;align-items:center;min-height:60vh;gap:15px}.button{display:inline-block;padding:15px 30px;background-color:#6c757d;color:#fff;text-decoration:none;border-radius:5px;font-size:16px;font-family:helvetica neue,sans-serif;text-align:center;width:200px;max-width:250px;transition:background-color .3s ease,transform .2s ease;box-shadow:0 2px 5px rgba(0,0,0,.1)}.button:hover{background-color:#5a6268;transform:scale(1.05)}.button:active{background-color:#545b62;transform:scale(.98)}.markdown-body pre{border:1px solid rgba(100,100,100,.5)}@media(prefers-color-scheme:dark){.markdown-body pre{border-color:rgba(255,255,255,.15)}}