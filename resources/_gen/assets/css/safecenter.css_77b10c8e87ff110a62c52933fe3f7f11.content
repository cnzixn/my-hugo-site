@charset "UTF-8";body{font-family:arial,sans-serif}.safecenter-page{max-width:500px;width:96%;margin:50px auto;padding:20px;border-radius:8px;box-shadow:0 4px 10px rgba(0,0,0,.1);border:1px solid #ccc}.safecenter-page header{margin-bottom:20px}.safecenter-page header h1{font-size:1.8rem;margin-bottom:10px;text-align:center}.safecenter-page header p{font-size:1rem;color:#666}.safecenter-page main{text-align:center}.safecenter-page .target-link{font-size:1.2rem;margin:20px 0}.safecenter-page .target-link span{color:#666;font-weight:700}.safecenter-page button{display:inline-block;padding:10px 100px;font-size:1.2rem;border:none;border-radius:5px;cursor:pointer;transition:background-color .3s ease}.safecenter-page .safecenter-button-ok{background-color:#007bff;color:#fff;margin:20px auto}.safecenter-page .safecenter-button-ok:hover{background-color:#0056b3}.safecenter-page .cf-turnstile{margin:20px auto;max-width:400px}.safecenter-page footer{margin-top:20px;font-size:.9rem;color:#888}.safecenter-page footer a{color:#007bff;text-decoration:none}.safecenter-page footer a:hover{text-decoration:underline}@media(max-width:768px){.safecenter-page header h1{font-size:1.5rem}.safecenter-page p{font-size:1rem}}